{
  "name": "Search Events in Text",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Search events in Text and redirect to Google Calendar",
  "browser_action": { "default_icon": "icon.png" },
  "icons": { "48": "icon_48.png", "128": "icon.png" },
  "background": {
    "scripts": [ "events.js", "background.js" ]
  },
  "permissions": [ "tabs", "http://*/*", "https://*/*" ]
}